{
    "name": "zanui/zanui-fixtures-bundle",
    "type": "symfony-bundle",
    "description": "Provides classes to easily load fixtures to the Doctrine ORM using YAML files",
    "keywords": ["persistence", "fixture", "doctrine", "yaml"],
    "homepage": "https://github.com/zanui/ZanuiFixturesBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Zanui Engineering Team",
            "email": "engineering@zanui.com.au"
        },
        {
            "name": "Javier Cejudo",
            "email": "javier.cejudo@zanui.com.au"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "symfony/finder": "~2.2",
        "symfony/yaml": "~2.2"
    },
    "require-dev": {
      "phpspec/phpspec": "~2.0",
      "henrikbjorn/phpspec-code-coverage": "~0.2",
      "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
          "Zanui\\FixturesBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "fixtures\\": "fixtures/"
        }
    },
    "config": {
      "bin-dir": "bin/"
    }
}
